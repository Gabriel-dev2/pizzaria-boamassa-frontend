{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "{{IMAGE_NAME}}",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "3000"
    }
  ],
  "Environment": [
    {
      "Name": "NODE_ENV",
      "Value": "production"
    },
    {
      "Name": "NEXT_PUBLIC_API_URL",
      "Value": "{{API_URL}}"
    }
  ]
}